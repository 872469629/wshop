/* pages/time/time.wxss */
page{
  background: #f6f4f4
}
.team-one{
  width: 90%;
  height: 200rpx;
  background: white;
  margin: 15rpx auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 5px;
  margin-bottom: 15rpx
}
.cj{
  color: #989898;
  font-size: 35rpx
}
.pr{
  font-size: 55rpx;
  color: #e07046
}

.team-two{
  width: 90%;
  background: white;
  margin: auto;
  border-radius: 5px;
}

.one{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #eaeaea
}
.left{
  width: 50%;
  height: 100%;
  float: left;
  text-indent: 30rpx;
  font-size: 32rpx
}
.right{
  width: 50%;
  height: 100%;
  float: right;
  display: flex;
  justify-content: center;
  align-items: center
}
.right text{
  width: 130rpx;
  text-align: center;
  font-size: 32rpx
}
.two{
  width: 100%;
  background: yellow
}

.two-box{
  width: 100%;
  height: auto;
  border-bottom: 1px solid #eaeaea;
  background: white;
  display: flex;
  flex-wrap: wrap
}
.two-box-box{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  margin: 15rpx 30rpx;
  width: 160rpx; 
  
}
.two-box image{
  width: 107rpx;
  height: 107rpx;
 
}
.two-box text{
  color: #ccc;
  font-size: 25rpx;
  margin-top: 10rpx
}






.on{
  color: #00b26a;
  border-bottom: 1px solid #00b26a
}