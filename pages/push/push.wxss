/* pages/time/time.wxss */
page{
  background: #f6f4f4
}
.team-one{
  width: 90%;
  height: 200rpx;
  background: white;
  margin: 15rpx auto;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 5px;
  margin-bottom: 15rpx
}
.cj{
  color: #989898;
  font-size: 35rpx
}
.pr{
  font-size: 55rpx;
  color: #e07046
}

.team-two{
  width: 90%;
  background: white;
  margin: auto;
  border-radius: 5px;
}

.one{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #eaeaea
}
.left{
  width: 50%;
  height: 100%;
  float: left;
  text-indent: 30rpx;
  font-size: 32rpx
}
.right{
  width: 50%;
  height: 100%;
  float: right;
  display: flex;
  justify-content: center;
  align-items: center
}
.right text{
  width: 130rpx;
  text-align: center;
  font-size: 32rpx
}

.two-box{
  width: 100%;
  height: 150rpx;
  border-bottom: 1px solid #eaeaea;
}
.two-box-left{
  width: 100rpx;
  height: 100rpx;
  float: left;
  border-radius: 50%;
  padding: 27rpx 20rpx
}
.two-box-left image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.two-box-con{
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  color: #999999;
  float: left
}
.two-box-con .t1{
  padding-top: 27rpx
}
.two-box-con .t2{

}

.two-box-right{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: right
}
.value{
  color: #707070;
  font-size: 27rpx;
  padding-right: 20rpx;
}
.price{
   font-size: 30rpx;
   color: #e07046;
   padding-right: 20rpx
}






.on{
  color: #00b26a;
  border-bottom: 1px solid #00b26a
}